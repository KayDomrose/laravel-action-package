{
    "name": "kaydomrose/laravel-api-action",
    "description": "This package simplifies creating api (json) endpoints for your laravel application. ",
    "license": "MIT",
    "authors": [
        {
            "name": "Kay Domrose",
            "email": "kay@domrose.org",
            "homepage": "https://kay.domrose.org"
        }
    ],
    "homepage": "https://github.com/kaydomrose/laravel-api-action",
    "keywords": ["Laravel", "LaravelApiAction"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Kaydomrose\\LaravelApiAction\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaydomrose\\LaravelApiAction\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaydomrose\\LaravelApiAction\\LaravelApiActionServiceProvider"
            ],
            "aliases": {
                "LaravelApiAction": "Kaydomrose\\LaravelApiAction\\Facades\\LaravelApiAction"
            }
        }
    }
}
